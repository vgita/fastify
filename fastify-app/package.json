{
    "name": "fastify-app",
    "version": "1.0.0",
    "description": "This project was bootstrapped with Fastify-CLI.",
    "main": "app.js",
    "directories": {
        "test": "test"
    },
    "scripts": {
        "lint": "standard",
        "lint:fix": "standard --fix",
        "pretest": "npm run lint",
        "test": "tap test/**/*.test.js --no-check-coverage",
        "start": "fastify start -l info --options app.js",
        "dev": "npm run start -- --watch --pretty-logs --debug",
        "mongo:start": "docker run -d -p 27017:27017 --rm --name fastify-mongo mongo:latest",
        "mongo:stop": "docker container stop fastify-mongo"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@fastify/autoload": "^6.1.0",
        "@fastify/cors": "^10.0.2",
        "@fastify/env": "^5.0.2",
        "@fastify/mongodb": "^9.0.2",
        "@fastify/sensible": "^6.0.3",
        "@fastify/swagger": "^9.4.2",
        "@fastify/swagger-ui": "^5.2.1",
        "fastify": "^5.2.1",
        "fastify-cli": "^7.3.0",
        "fastify-plugin": "^5.0.1"
    },
    "devDependencies": {
        "standard": "^17.1.2",
        "tap": "^21.0.1"
    }
}
