{
    "type": "object",
    "$id": "schema:dotenv",
    "required": ["MONGO_URL"],
    "properties": {
        "NODE_ENV": {
            "type": "string",
            "default": "development"
        },
        "PORT": {
            "type": "integer",
            "default": 3000
        },
        "MONGO_URL": {
            "type": "string"
        },
        "JWT_SECRET": {
            "type": "string",
            "default": "superS3cr3t"
        },
        "LOG_LEVEL": {
            "type": "string",
            "default": "warn"
        },
        "REQUEST_ID_LOG_LABEL": {
            "type": "string",
            "default": "reqId"
        },
        "REQUEST_ID_HEADER": {
            "type": "string",
            "default": "x-request-id"
        }
    }
}
